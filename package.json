{
  "name": "hometask_1-videos",
  "version": "1.0.0",
  "description": "Express REST API backend course pet-project videos",
  "type": "commonjs",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "build:start": "yarn build && yarn start",
    "dev": "nodemon --watch src --ext ts,tsx --exec \"node --inspect=9229 --enable-source-maps --import tsx src/server.ts\"",
    "test": "jest",
    "test:e2e": "jest src/__tests__/e2e",
    "lint": "eslint --fix .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edward-tobilko/backend-course.git"
  },
